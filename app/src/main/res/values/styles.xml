<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="get_your_gr">
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">#030303</item>
        <item name="fontFamily">@font/gilda_display</item>
    </style>

    <style name="enter_your_">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">#181725</item>

    </style>

    <style name="mobile_numb">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#7C7C7C</item>
    </style>

    <style name="select_your">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#8E8CA2</item>
    </style>

    <style name="textTheme">
        <item name="android:textSize">@dimen/_13sdp</item>
        <item name="android:textColor">@color/filledBlack</item>
    </style>

    <style name="Theme.Transparent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="bigTextTheme">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#201E1E</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="exclusive_o">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#181725</item>
    </style>

</resources>